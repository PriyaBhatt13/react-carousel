    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-jest": "^9.0.0",
    "babel-polyfill": "*",
    "jest-cli": "*",
    "react-addons-test-utils": "~0.14.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
